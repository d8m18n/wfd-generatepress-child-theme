  "license": "ISC",
  "dependencies": {
    "@wordpress/scripts": "^18.0.0"
  },
  "devDependencies": {
    "@wordpress/base-styles": "^4.0.0",
    "@wordpress/browserslist-config": "^4.1.0",
    "@wordpress/eslint-plugin": "^9.1.2",
    "@wordpress/stylelint-config": "^19.1.0",
    "browser-sync": "^2.27.5",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
